{
  "name": "linkedin-bot",
  "version": "1.0.0",
  "description": "A Puppeteer script to open LinkedIn with React UI",
  "type": "module",
  "scripts": {
    "server": "tsx src/server/index.ts",
    "client": "vite",
    "build:client": "vite build",
    "dev": "concurrently \"npm run server\" \"npm run client\"",
    "build": "npm run build:client && tsc --project tsconfig.server.json"
  },
  "keywords": [
    "puppeteer",
    "linkedin",
    "automation",
    "react"
  ],
  "author": "",
  "license": "ISC",
  "dependencies": {
    "express": "^4.18.2",
    "puppeteer": "^24.29.1",
    "ws": "^8.14.2",
    "cors": "^2.8.5"
  },
  "devDependencies": {
    "@types/express": "^4.17.20",
    "@types/node": "^20.9.0",
    "@types/react": "^18.2.37",
    "@types/react-dom": "^18.2.15",
    "@types/ws": "^8.5.8",
    "@types/cors": "^2.8.15",
    "@vitejs/plugin-react": "^4.2.0",
    "concurrently": "^8.2.2",
    "react": "^18.2.0",
    "react-dom": "^18.2.0",
    "tsx": "^4.7.0",
    "typescript": "^5.2.2",
    "vite": "^5.0.0"
  }
}
